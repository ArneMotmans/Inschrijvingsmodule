{
    "extends": ["../tslint.json", "tslint:recommended", "tslint-sonarts", "tslint-consistent-codestyle"],
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-consecutive-blank-lines":true,
        "no const-parameters": true,
        "no-unused": true,
        "no-unused-variable": true,
        "no-unused-expression": true,

    }
}
